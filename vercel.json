{
  "version": 2,
  "builds": [
    {
      "src": "api/health.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/run-scraper.py", 
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.py"
    },
    {
      "src": "/api/run-scraper",
      "dest": "/api/run-scraper.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 